<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentContainer">

    <fragment
        android:id="@+id/fragmentContainer"
        android:name="com.example.scheduleapp.UI.FragmentContainer"
        android:label="FragmentContainer"
        tools:layout="@layout/fragment_container"
        >
        <action
            android:id="@+id/action_fragmentContainer_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.scheduleapp.UI.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
</navigation>